{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "build/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mcp",
      "dest": "/build/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.js"
    }
  ]
}
